@model AA.HRMS.Interfaces.IEducationHistoryView

@{

                ViewBag.Title = "Add Education History";
}

@Html.ValidationSummary()
@Model.ProcessingMessage

<div class="modal-header">
    <h3>@ViewBag.Title</h3>
    <button type="button" class="close" data-dismiss="modal">&times;</button>
</div>

@using (Html.BeginForm("Create", "EducationHistory", FormMethod.Post, new { id = "AddEducationHistoryForm" }))
{
    @Html.AntiForgeryToken()

<div class="modal-body">


    <input name="url" id="url" value="@Model.URL" hidden />
    <input name="employeeId" id="employeeId" value="@Model.EmployeeId" hidden />

    <div class="form-example-int">
        <div class="form-group">
            <label>School Name</label>
            <div class="">
                <input id="SchoolName" name="SchoolName" type="text" class="form-control" value="@Model.SchoolName" />
            </div>
        </div>
    </div>

    <div class="form-example-int">
        <div class="form-group">
            <label>Degree</label>
            <div class="">
                <input id="Degree" name="Degree" type="text" class="form-control" value="@Model.Degree" />
            </div>
        </div>
    </div>

    <div class="form-example-int">
        <div class="form-group">
            <label>Duration</label>
            <div class="">
                <input id="graduationYear" name="graduationYear" type="number" class="form-control" value="@Model.GraduationYear" />
            </div>
        </div>
    </div>

    <div class="form-example-int">
        <div class="form-group">
            <label>Note</label>
            <div class="">
                <textarea id="Note" name="Note" class="form-control" value="@Model.Note" type="text" maxlength="50">@Model.Note</textarea>
            </div>
        </div>
    </div>

</div>

    <div class="modal-footer">
        <button type="submit" name="submitButton" class="btn btn-default">Add</button>
        <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
    </div>
}




