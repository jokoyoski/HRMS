
@model AA.HRMS.Interfaces.IEmploymentHistoryView

@{
    ViewBag.Title = "Delete Employment History";
}

<div class="modal-header">
    <h3>@ViewBag.Title</h3>
    <button type="button" class="close" data-dismiss="modal">&times;</button>
</div>



@using (Html.BeginForm("Delete", "EmploymentHistory", FormMethod.Post, new { id = "EmploymentHistoryForm", enctype = "multipart/form-data", encoding = "multipart/form-data" }))
{
    Html.AntiForgeryToken();



<div class="modal-body">


    <input type="hidden" id="employmentHistoryId" name="employmentHistoryId" value="@Model.EmploymentHistoryId" />
    <input type="hidden" id="URL" name="URL" value="@Model.URL" />

    @if (!string.IsNullOrEmpty(Model.ProcessingMessage))
    {

        <div class="alert alert-info alert-dismissible" role="alert">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true"><i class="notika-icon notika-close"></i></span></button> @Model.ProcessingMessage
        </div>
    }

    @if (!ViewData.ModelState.IsValid)
    {
        <div class="alert alert-danger alert-dismissible" role="alert">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true"><i class="notika-icon notika-close"></i></span></button> @Html.ValidationSummary()
        </div>
    }

    <div class="form-example-int">
        <div class="form-group">
            <label>Company Name</label>
            <div class="nk-int-st">
                <label id="companyName" name="companyName" style="width: 250px;" type="text" maxlength="50">@Model.CompanyName</label>
            </div>
        </div>
    </div>
    <div class="form-example-int">
        <div class="form-group">
            <label>job Function</label>
            <div class="nk-int-st">
                <label id="jobFunction" name="jobFunction" style="width: 250px;" type="text" maxlength="50">@Model.JobFunction</label>
            </div>
        </div>
    </div>
    <div class="form-example-int">
        <div class="form-group">
            <label>start Date</label>
            <div class="nk-int-st">
                <label id="startDate" name="startDate" style="width: 250px;" type="text" maxlength="50">@Model.StartDate</label>
            </div>
        </div>
    </div>
    <div class="form-example-int">
        <div class="form-group">
            <label>End Date</label>
            <div class="nk-int-st">
                <label id="EndDate" name="EndDate" style="width: 250px;" type="text" maxlength="50">@Model.EndDate</label>
            </div>
        </div>
    </div>
</div>

            <div class="modal-footer">
                <button type="submit" name="submitButton" class="btn btn-default">Delete</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
            </div>



        }

@section JavaScriptsIncludes
{
    <script src="/Scripts/jquery-ui-1.12.1.js" type="text/javascript"></script>
}

@section JavaScriptsExecution
{

}
            
