@model AA.HRMS.Interfaces.ISkillSetModelView

@{
    ViewBag.Title = "Add Skill Set";
}



@Html.ValidationSummary()
@Model.ProcessingMessage


<div class="modal-header">
    <h3>@ViewBag.Title</h3>
    <button type="button" class="close" data-dismiss="modal">&times;</button>
</div>

@using (Html.BeginForm("Create", "SkillSet", FormMethod.Post, new { id = "AddSkillSetForm" }))
{
    @Html.AntiForgeryToken()


<div class="modal-body">

    <input name="url" id="url" value="@Model.URL" hidden />
    <input name="employeeId" id="employeeId" value="@Model.EmployeeId" hidden />

    <div class="form-example-int">
        <div class="form-group">
            <label>Skill Name</label>
            <div class="">
                <input id="skillName" name="skillName" type="text" class="form-control" value="@Model.SkillName" />
            </div>
        </div>
    </div>
        <div class="form-example-int">
        <div class="form-group">
            <label>Skill Description</label>
            <div class="">
                <input id="skillDescription" name="skillDescription" type="text" class="form-control" value="@Model.SkillDescription" />
            </div>
        </div>
    </div>

        <div class="form-example-int">
        <div class="form-group">
            <label>Years of Experience</label>
            <div class="">
                @Html.DropDownList("Experience", Model.ExperienceDropDown, new { @class = "form-control", id = "Experience" })
            </div>
            </div>
        </div>

  </div>
      
            <div class="modal-footer">
                <button type="submit" name="submitButton" class="btn btn-default">Add</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
            </div>
    }
   


            